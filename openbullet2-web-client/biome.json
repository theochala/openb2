{
  "formatter": {
    "enabled": true,
    "lineWidth": 120,
    "indentStyle": "space",
    "indentWidth": 2
  },
  "javascript": {
    "formatter": {
      "quoteStyle": "single"
    }
  },
  "linter": {
    "rules": {
      "style": {
        "useImportType": "off",
        "noNonNullAssertion": "off"
      },
      "complexity": {
        "useLiteralKeys": "off"
      }
    }
  }
}
